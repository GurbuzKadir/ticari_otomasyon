@model TicariOtomasyon.Models.Siniflar.Yapilacak
@{
    ViewBag.Title = "YeniYapilacak";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br />
<h2>Yeni Yapılacak Ekle</h2>
<br />
<form class="form-group" method="post">

    <p>Başlık</p>
    @Html.TextBoxFor(x => x.Baslik, new { @class = "form-control" })

    <br />
    
    <p>Detay</p>
    @Html.TextBoxFor(x => x.Detay, new { @class = "form-control" })

    <br />

    <button class="btn btn-success">Kaydet</button>
</form>